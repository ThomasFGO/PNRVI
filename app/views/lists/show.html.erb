<div class="container">

  <%= render partial: "lists/header" %>

  <!-- Nav tabs -->
  <ul class="nav nav-tabs" id="myTab" role="tablist">
    <li class="nav-item">
      <a class="nav-link active" id="visuals-tab" data-toggle="tab" href="#visuals" role="tab" aria-controls="visuals" aria-selected="true">
        Visuels
      </a>
    </li>
    <li class="nav-item">
      <a class="nav-link" id="listing-tab" data-toggle="tab" href="#listing" role="tab" aria-controls="listing" aria-selected="false">
        Liste
      </a>
    </li>
  </ul>

  <!-- Tab panes -->
  <div class="tab-content">
    <div class="tab-pane active" id="visuals" role="tabpanel" aria-labelledby="visuals-tab">
      <%= render partial: "lists/visuals" %>
    </div>

    <div class="tab-pane" id="listing" role="tabpanel" aria-labelledby="listing-tab">
      <%= render partial: "lists/listing" %>
    </div>
  </div>
</div>

